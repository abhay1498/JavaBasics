import java.util.Scanner;

public class DigitCount {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int count=0;
		int num=sc.nextInt();
		while(num!=0) {
			System.out.println(num=num/10);
			
			count++;
		}
		System.out.println(count);
	}

}
